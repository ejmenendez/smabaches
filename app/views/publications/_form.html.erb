<%= form_for @publication do |form| %>
	
	<p>
        <%= form.label :decription %><br>
        <%= form.text_field :description %>
	</p>

	<p>
        <%= form.label :latitude %><br>
        <%= form.text_field :latitude %>
	</p>

    <p>
        <%= form.label :longitude %><br>
        <%= form.text_field :longitude %>
	</p>
	
	<p>
		<%= form.submit %>
	</p>

<% end %>